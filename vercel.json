{
  "redirects": [
    {
      "source": "/",
      "destination": "https://openlive.cc/",
      "statusCode": 301
    },
    {
      "source": "/:path*",
      "destination": "https://openlive.cc/:path*",
      "statusCode": 301
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex, nofollow"
        }
      ]
    }
  ]
}
